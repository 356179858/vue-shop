onent: login\r\n}, {\r\n    path: '/home',\r\n    name: 'home',\r\n    component: home,\r\n    // redirect: '/welcome',\r\n    children: [{\r\n        path: 'welcome',\r\n        component: welcome\r\n    }, {\r\n        path: '/users',\r\n        component: users\r\n    }, {\r\n        path: '/roles',\r\n        component: roles\r\n    }, {\r\n        path: '/rights',\r\n        component: powerList\r\n    }, {\r\n        path: '/goods',\r\n        component: goodsList\r\n    }, {\r\n        path: '/params',\r\n        component: goodsData\r\n    }, {\r\n        path: '/categories',\r\n        component: goodsType\r\n    }, {\r\n        path: '/orders',\r\n        component: orderList\r\n    }, {\r\n        path: '/reports',\r\n        component: dataForm\r\n    }]\r\n}]\r\nconst originalPush = VueRouter.prototype.push\r\nVueRouter.prototype.push = function push(location) {\r\n    return originalPush.call(this, location).catch(err => err)\r\n}\r\nconst router = new VueRouter({\r\n    mode: 'history',\r\n    base: process.env.BASE_URL,\r\n    routes\r\n})\r\n\r\n//拦截路由导航守卫\r\nrouter.beforeEach((to, from, next) => {\r\n    if (to.path === '/login') return next()\r\n    //回去token\r\n    const tokenStr = window.sessionStorage.getItem('token')\r\n    if (!tokenStr) return next('/login')\r\n    next()\r\n})\r\n\r\nexport default router\n"]}]}