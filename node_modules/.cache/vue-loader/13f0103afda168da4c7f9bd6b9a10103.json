-for=\"item in menuList\" :key=\"item.id\">\r\n            <!-- 一级菜单模板区域 -->\r\n            <template slot=\"title\">\r\n              <!-- 图标 -->\r\n              <i :class=\"iconsObj[item.id]\"></i>\r\n              <!-- 文本 -->\r\n              <span>{{item.authName}}</span>\r\n            </template>\r\n            <!-- 二级菜单 -->\r\n            <el-menu-item :index=\"subItem.path + ''\" v-for=\"subItem in item.children\" :key='subItem.id'>\r\n              <!-- 二级菜单模板区域 -->\r\n              <template slot=\"title\">\r\n                <!-- 图标 -->\r\n                <i class=\"el-icon-menu\"></i>\r\n                <!-- 文本 -->\r\n                <span>{{subItem.authName}}</span>\r\n              </template>\r\n            </el-menu-item>\r\n          </el-submenu>\r\n\r\n        </el-menu>\r\n      </el-aside>\r\n      <el-main><router-view></router-view></el-main>\r\n    </el-container>\r\n  </el-container>\n\r\n</template>\r\n\r\n<script>\r\n  // import {\r\n  //   GoodsData\r\n  // } from \"network/home\";\r\n\r\n  export default {\r\n    name: 'Home',\r\n    data() {\r\n      return {\r\n        //左侧菜单\r\n        menuList: [],\n        color:{\n          activeColor:'#409bff',\n          textColor:'#fff',\n          backgroundColor:'#333744'\n        },\n        iconsObj:{\n          '125':'el-icon-user-solid',\n          '103':'el-icon-s-help',\n          '101':'el-icon-s-goods',\n          '102':'el-icon-s-order',\n          '145':'el-icon-s-marketing'\n        }\r\n      }\r\n    },\r\n    created() {\r\n      this.getMenuList()\r\n    },\r\n    methods: {\r\n      logout() {\r\n        window.sessionStorage.removeItem(\"token\")\r\n        this.$router.push('/login')\r\n      },\r\n      // 获取所有菜单\r\n      async getMenuList() {\r\n        const {\r\n          data: res\r\n        } = await this.$http.get('menus')\r\n        if (res.meta.status !== 200) return this.$message.error(res.meta.msg)\r\n        this.menuList = res.data\r\n        console.log(res);\